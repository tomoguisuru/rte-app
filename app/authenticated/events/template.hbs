<h3>Events:</h3>

<ul>
{{#each model as |event|}}
    <li>
        {{event.title}}
        {{#if (or (eq event.state 'staging') (eq event.state 'live'))}}
        <a href="/events/{{event.id}}">View</a>
        <a href="/events/{{event.id}}/publisher/{{event.stream.id}}">Join</a>
        {{/if}}
    </li>
{{else}}
    No Events
{{/each}}
</ul>